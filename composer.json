{
    "name": "zendframework/skeleton-yaml",
    "description": "Skeleton Application for ZF2",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "zf2"
    ],
    "homepage": "http://framework.zend.com/",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.*@stable",
        "zendframework/zend-developer-tools": "dev-master",
        "zf-commons/zfc-twig": "dev-master",
        "zendframework/zftool": "dev-master",
        "spiffy/spiffy-routes": "dev-master",
        "hounddog/doctrine-data-fixture-module": "dev-master",
        "doctrine/doctrine-orm-module": "0.8.0",
        "doctrine/doctrine-module": "0.8.*",
        "doctrine/mongodb-odm": "dev-master",
        "doctrine/doctrine-mongo-odm-module": "dev-master",
        "doctrine/data-fixtures": "dev-master",
        "gedmo/doctrine-extensions": "dev-master",
        "sundrique/doctrine-tools": ">=0.2",
        "widmogrod/zf2-assetic-module": "1.*",
        "twbs/bootstrap": "3.*@stable",
        "fortawesome/font-awesome": "4.0.*",
        "components/jquery": "1.10.*",
        "malsup/form": "dev-master",
        "components/jqueryui": "dev-master"
    },
   "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4":{
            "Assets\\" : "module/Assets/src/"
        }
    }
}
