{
    "name": "zendframework/skeleton-yaml",
    "description": "Skeleton Application for ZF2",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "zf2"
    ],
    "homepage": "http://framework.zend.com/",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.*@stable",
        "zf-commons/zfc-twig": "dev-master",
        "spiffy/spiffy-routes": "dev-master",
        "widmogrod/zf2-assetic-module": "1.*",
        "twbs/bootstrap": "3.*@stable",
        "fortawesome/font-awesome": "4.0.*",
        "components/jqueryui": "dev-master",
        "components/jquery": "1.10.*",
        "malsup/form": "dev-master"
    },
   "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [ "pwd" ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "": "module/" },
        "psr-4": {
            "Assets\\" : "module/Assets/src/",
            "Application\\" : "module/Application/src/"
        }
    }
}
